
{
  "isDev": true,
  "http": {
      "baseUrl": "http://0.0.0.0",
      "port": 8000,
      "key": "/etc/prosody/certs/{{XMPP_SERVER_URL}}.key",
      "cert": "/etc/prosody/certs/{{XMPP_SERVER_URL}}.crt"
  },
  "session": {
      "secret": "{{XMPP_CLIENT_SECRET}}"
  },
  "server": {
      "name": "{{NAME}}",
      "domain": "{{XMPP_SERVER_URL}}",
      "wss": "wss://{{XMPP_SERVER_URL}}/_xmpp/websocket/",
      "sasl": "scram-sha-1",
      "securePasswordStorage": true,
      "muc": "{{XMPP_GROUPS_URL}}",
      "startup": "groupchat/lobby%40{{XMPP_GROUPS_URL}}",
      "admin": "admin",
      "keepalive": {
          "interval": 45,
          "timeout": 15
      }
  }
}